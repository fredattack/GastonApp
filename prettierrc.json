{
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "eslintIntegration": true,
    "htmlWhitespaceSensitivity": "strict",
    "insertPragma": true,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "parser": "babel",
    "printWidth": 80,
    "proseWrap": "always",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": false,
    "singleAttributePerLine": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": false,
    "vueIndentScriptAndStyle": false
}
